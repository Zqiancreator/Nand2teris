/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
 //a&!sel | b&sel
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
	And(a=b,b=sel,out=bsel);
	Not(in=sel,out=nsel);
	And(a=a,b=nsel,out=ansel);
	Or(a=bsel,b=ansel,out=out);
}